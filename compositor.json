{
  "name": "giocodes/portfolio",
  "version": "0.1.4",
  "libraries": {
    "xv": "^1.1.25"
  },
  "title": "Personal Website",
  "branch": "",
  "style": {
    "name": "Future",
    "componentSet": {
      "nav": "nav/BasicNav",
      "header": "header/BannerHeader",
      "article": "article/BasicArticle",
      "footer": "footer/BasicFooter"
    },
    "fontFamily": "\"Avenir Next\", Helvetica, sans-serif",
    "heading": {
      "fontWeight": 500,
      "textTransform": "uppercase",
      "letterSpacing": "0.2em"
    },
    "alternativeText": {
      "textTransform": "uppercase",
      "fontWeight": 400,
      "letterSpacing": "0.2em",
      "opacity": 0.75
    },
    "colors": {
      "text": "#333",
      "background": "#fff",
      "primary": "#666",
      "secondary": "#888",
      "highlight": "#1f80ff",
      "muted": "#f6f6f6",
      "border": "#eee"
    },
    "layout": {
      "centered": true,
      "maxWidth": 1024,
      "bannerHeight": "80vh"
    }
  },
  "content": [
    {
      "component": "nav",
      "links": [
        {
          "href": "https://giocodes.github.io/portfolio/",
          "text": "Home"
        },
        {
          "href": "https://github.com/giocodes/portfolio",
          "text": "GitHub"
        },
        {
          "href": "https://npmjs.com/package/portfolio",
          "text": "npm"
        }
      ]
    },
    {
      "component": "header",
      "heading": "portfolio",
      "subhead": "I'm a the Bay Area based Full Stack developer",
      "children": [
        {
          "component": "ui/TweetButton",
          "text": "portfolio: I'm a the Bay Area based Full Stack developer",
          "url": "https://giocodes.github.io/portfolio/"
        },
        {
          "component": "ui/GithubButton",
          "user": "giocodes",
          "repo": "portfolio"
        }
      ],
      "text": "v0.1.0"
    },
    {
      "component": "article",
      "metadata": {
        "source": "github.readme"
      },
      "html": "<p>Content for my personal website &#x1F3A9;</p>\n"
    },
    {
      "component": "footer",
      "links": [
        {
          "href": "https://github.com/giocodes/portfolio",
          "text": "GitHub"
        },
        {
          "href": "https://github.com/giocodes",
          "text": "giocodes"
        }
      ]
    }
  ]
}